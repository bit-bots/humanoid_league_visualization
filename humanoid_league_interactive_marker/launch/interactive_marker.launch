<launch>
    <node name="rviz" pkg="rviz" type="rviz" output="screen"
          args="-d $(find-pkg-share humanoid_league_interactive_marker)/config/interactive_marker.rviz">
    </node>
    <node name="interactive_marker" pkg="humanoid_league_interactive_marker" type="interactive_marker.py" output="screen">
        <param from="$(find-pkg-share humanoid_league_interactive_marker)/config/camera_info.yaml" />
    </node>
</launch>